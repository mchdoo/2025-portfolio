#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/pkg-utils/bin/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/pkg-utils/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/pkg-utils/bin/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/pkg-utils/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/@sanity+pkg-utils@6.13.4_@types+babel__core@7.20.5_@types+node@22.13.5_debug@4.4.0_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/pkg-utils/bin/pkg-utils.cjs" "$@"
else
  exec node  "$basedir/../@sanity/pkg-utils/bin/pkg-utils.cjs" "$@"
fi
