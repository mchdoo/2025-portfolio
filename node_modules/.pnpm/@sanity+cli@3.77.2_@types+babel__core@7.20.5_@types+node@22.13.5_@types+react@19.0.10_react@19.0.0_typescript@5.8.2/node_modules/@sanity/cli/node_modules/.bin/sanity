else
  export NODE_PATH="/Users/pedro/Developer/graphs/test-p5/node_modules/.pnpm/@sanity+cli@3.77.2_@types+babel__core@7.20.5_@types+node@22.13.5_@types+react@19.0.10_react@19.0.0_typescript@5.8.2/node_modules/@sanity/cli/bin/node_modules:/Users/pedro/Developer/graphs/test-p5/node_modules/.pnpm/@sanity+cli@3.77.2_@types+babel__core@7.20.5_@types+node@22.13.5_@types+react@19.0.10_react@19.0.0_typescript@5.8.2/node_modules/@sanity/cli/node_modules:/Users/pedro/Developer/graphs/test-p5/node_modules/.pnpm/@sanity+cli@3.77.2_@types+babel__core@7.20.5_@types+node@22.13.5_@types+react@19.0.10_react@19.0.0_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/test-p5/node_modules/.pnpm/@sanity+cli@3.77.2_@types+babel__core@7.20.5_@types+node@22.13.5_@types+react@19.0.10_react@19.0.0_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/test-p5/node_modules/.pnpm/node_modules:$NODE_PATH"
