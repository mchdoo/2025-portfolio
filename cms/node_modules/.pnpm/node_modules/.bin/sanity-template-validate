#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/template-validator/dist/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/template-validator/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/template-validator/dist/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/template-validator/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+template-validator@2.4.0_@types+babel__core@7.20.5_@types+node@22.13.9_debug@4.4.0_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/template-validator/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../@sanity/template-validator/dist/cli.cjs" "$@"
fi
