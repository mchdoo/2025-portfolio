else
  export NODE_PATH="/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+cli@3.78.0_@types+babel__core@7.20.5_@types+node@22.13.9_@types+react@18.3.18_react@18.3.1_typescript@5.8.2/node_modules/@sanity/cli/bin/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+cli@3.78.0_@types+babel__core@7.20.5_@types+node@22.13.9_@types+react@18.3.18_react@18.3.1_typescript@5.8.2/node_modules/@sanity/cli/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+cli@3.78.0_@types+babel__core@7.20.5_@types+node@22.13.9_@types+react@18.3.18_react@18.3.1_typescript@5.8.2/node_modules/@sanity/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/@sanity+cli@3.78.0_@types+babel__core@7.20.5_@types+node@22.13.9_@types+react@18.3.18_react@18.3.1_typescript@5.8.2/node_modules:/Users/pedro/Developer/graphs/portfolio/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
